{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.yaml",
    "src/**/*.tsx",
    "src/**/*.json"
  ],
  "exclude": [
    "node_modules"
  ],
  
  
}